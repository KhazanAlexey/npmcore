{
  "name": "library2",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "library2/src",
  "projectType": "library",
  "tags": ["test lib"],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["library2/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/library2"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["coverage/library2"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../coverage/library2"
      }
    }
  }
}
